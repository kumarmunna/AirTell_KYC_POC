pipeline {
    agent any
    stages {
        stage('Building Stage') {
            steps {
                echo "Running project build ... "
                checkout scm
                sh 'make'
                stash includes: '**/target/*.jar', name: 'userregistration'
            }
        }
        stage('Testing Stage') {
            steps {
                echo "Running project testing ... "
            }
        }
        stage('Deploying Stage') {
            steps {
                echo "Running project deploying ... "
            }
        }
    }
}
